<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <CheckBox
        android:id="@+id/read_did_result_code_cb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/result_code" />
    <Spinner
        android:id="@+id/read_did_result_code_spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <CheckBox
        android:id="@+id/read_did_result_cb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/did_result" />
    <Button
        android:id="@+id/create_did_result_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/create_did_result" />
    <ListView
        android:id="@+id/read_did_result_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>